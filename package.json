{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "chart.js": "^2.7.2",
    "hover.css": "^2.3.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.5",
    "react": "^16.5.2",
    "react-animated-css": "^1.0.4",
    "react-chartjs-2": "^2.7.4",
    "react-delay": "^0.1.0",
    "react-delay-render": "^0.1.2",
    "react-dom": "^16.8.3",
    "react-dropzone": "^10.1.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-spinners": "^0.5.3",
    "react-table": "^6.10.0",
